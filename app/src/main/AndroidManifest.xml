<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.erej.a40k9thScoring">
    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.erej.a40k9thScoring.MainActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.erej.a40k9thScoring.CreateBattleFast" />
        <activity android:name="com.erej.a40k9thScoring.CreateBattleSlow"/>
        <activity android:name="com.erej.a40k9thScoring.PlayGameActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"/>
        <activity android:name=".SettingsActivity" />
        <activity android:name=".UpdateDataPopUp" />

    </application>

</manifest>